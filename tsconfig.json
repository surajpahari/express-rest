{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./src/types/express/",
      "./node_modules/@types"
    ]
  },
  "include": [
    "src",
    "src/types/express/express.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
